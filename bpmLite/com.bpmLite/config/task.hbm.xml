<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="database.AssignedRoleModel" table="assigned_role">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="user">
			<column name="user" />
		</property>
		<property name="role">
			<column name="role" />
		</property>
		<property name="groupMembership">
			<column name="group_membership" />
		</property>
		<property name="userSkills">
			<column name="user_skils" />
		</property>
	</class>

	<class name="database.AuditModel" table="audit">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="action">
			<column name="action" />
		</property>
		<property name="user">
			<column name="user" />
		</property>
		<property name="stepId">
			<column name="step_id" />
		</property>
		<property name="stepId">
			<column name="step_id" />
		</property>
		<property name="startTime">
			<column name="start_time" />
		</property>
		<property name="completeTime">
			<column name="complete_time" />
		</property>
	</class>


	<class name="database.JoinStatusModel" table="join_status">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="stepId">
			<column name="step_id" />
		</property>
		<property name="stepOne">
			<column name="step_one" />
		</property>
		<property name="stepTwo">
			<column name="step_two" />
		</property>
		<property name="stepThree">
			<column name="step_three" />
		</property>
		<property name="stepCountRequired">
			<column name="step_count_required" />
		</property>
	</class>
	
	<class name="database.ProcessInstanceModel" table="process_instance">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="process">
			<column name="process" />
		</property>
		<property name="currentStep">
			<column name="current_step" />
		</property>
	</class>
	
	<class name="database.ProcessModel" table="process">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="name">
			<column name="name" />
		</property>
		<property name="version">
			<column name="version" />
		</property>
		<property name="uniqueGuid">
			<column name="unique_guid" />
		</property>
		<property name="startStep">
			<column name="start_step" />
		</property>
		<property name="fieldIds">
			<column name="field_ids" />
		</property>
	</class>	
	
	<class name="database.StepDataModel" table="step_data">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="stepId">
			<column name="step_id" />
		</property>
		<property name="procesId">
			<column name="process_id" />
		</property>
		<property name="stepType">
			<column name="step_type" />
		</property>
		<property name="stepDetails">
			<column name="step_details" />
		</property>
		<property name="userList">
			<column name="user_list" />
		</property>
		<property name="nextId">
			<column name="next_id" />
		</property>
		<property name="previousId">
			<column name="previous_id" />
		</property>
		<property name="fieldData">
			<column name="field_data" />
		</property>
		<property name="globalData">
			<column name="global_data" />
		</property>
	</class>	
	
	<class name="database.UserGroupsModel" table="user_groups">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="name">
			<column name="name" />
		</property>
		<property name="description">
			<column name="description" />
		</property>
	</class>

	<class name="database.UserModel" table="user">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="name">
			<column name="name" />
		</property>
		<property name="description">
			<column name="description" />
		</property>
		<property name="uniqueKey">
			<column name="unique_key" />
		</property>
		<property name="startTime">
			<column name="description" />
		</property>
	</class>
	
	<class name="database.UserSkillsModel" table="user_skills">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="name">
			<column name="name" />
		</property>
		<property name="description">
			<column name="description" />
		</property>
		<property name="uniqueKey">
			<column name="unique_key" />
		</property>
		<property name="startTime">
			<column name="start_time" />
		</property>
	</class>

</hibernate-mapping>